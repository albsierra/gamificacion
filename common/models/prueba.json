{
  "name": "Prueba",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "nombre": {
      "type": "string",
      "required": true
    },
    "descripcion": {
      "type": "string"
    },
    "maximo": {
      "type": "number"
    }
  },
  "validations": [],
  "relations": {
    "juego": {
      "type": "belongsTo",
      "model": "Juego",
      "foreignKey": ""
    },
    "grupos": {
      "type": "hasMany",
      "model": "Grupo",
      "foreignKey": "",
      "through": "Marcador"
    },
    "imagen": {
      "type": "hasOne",
      "model": "Imagen",
      "polymorphic": {
        "foreignKey": "imageableId",
        "discriminator": "imageableType"
      }
    },
    "puntuaciones": {
      "type": "hasMany",
      "model": "Marcador",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "coordinadorJuego",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "ranking"
    }
  ],
  "methods": {
    "prototype.ranking": {
      "accepts": [],
      "returns": [
        {
          "arg": "puntosEquipos",
          "type": "array",
          "root": false,
          "description": "Una array conteniendo el nombre y puntuaci√≥n de los equipos en la prueba consultada."
        }
      ],
      "description": "Puntuaciones de los equipos en una prueba determinada",
      "http": [
        {
          "path": "/ranking",
          "verb": "get"
        }
      ]
    }
  }
}
